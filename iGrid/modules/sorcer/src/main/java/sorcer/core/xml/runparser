java sorcer.service.xml.ServiceJobXMLDomParser -s '<?xml version ="1.0" encoding="UTF-8"?>


<!DOCTYPE ServiceJob [
<!ELEMENT ServiceJob (ServiceContext+, ServiceTask+)> 
<!ATTLIST ServiceJob 
	name CDATA #REQUIRED
>
<!ELEMENT ServiceContext (DataNode | TextNode)*>
<!ATTLIST ServiceContext
	name CDATA #REQUIRED
>
<!ELEMENT DataNode (#PCDATA)>
<!ATTLIST DataNode
	path CDATA #REQUIRED
	NodeType CDATA #REQUIRED
	ioType CDATA "IN_TYPE"
	isTransient CDATA "true"
	data CDATA #REQUIRED
>
<!ELEMENT DataNode (#PCDATA)>
<!ATTLIST DataNode
	path CDATA #REQUIRED
	NodeType CDATA #REQUIRED
	ioType CDATA "IN_TYPE"
	isTransient CDATA "true"
	data CDATA #REQUIRED
>
<!ELEMENT TextNode (#PCDATA)>
<!ATTLIST TextNode
	path CDATA #REQUIRED
>
<!ATTLIST ServiceTask
	name CDATA #REQUIRED
	description CDATA ""
	method CDATA #REQUIRED
	provider CDATA #REQUIRED
	
>
<!ELEMENT TaskContext (#PCDATA)>
<!ATTLIST TaskContext
	root CDATA #REQUIRED
>
]>




<ServiceJob name="test job">

   <ServiceContext name="DAS">
      <DataNode path="GEOM/SHANK/MESH/DATA" ioType="IN_TYPE" NodeType="SORCER_DATA_MESH" 
