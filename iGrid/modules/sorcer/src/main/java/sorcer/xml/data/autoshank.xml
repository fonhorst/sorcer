<?xml version ="1.0" encoding="UTF-8"?>

<!DOCTYPE FiperJob SYSTEM "fiperjob.dtd">


<FiperJob name="autoshank job">

  <FiperContext name="shank">
    <DataNode path="GEOM/SHANK/MESH/DATA" ioType="IN_TYPE" fiperType="FIPER_DATA_MESH" 
      data="http://hippolyta.cs.ttu.edu/fiper/data/Shank_Tet_Mesh.dat">  TET </DataNode>
    <DataNode path="GEOM/SHANK/SHANK_PRT" ioType="IN_TYPE" fiperType="FIPER_AUTOSHANK_SEED" 
      data="http://hippolyta.cs.ttu.edu/fiper/data/AS_Shank_Prt.prt"> SEEDPART </DataNode>
    <DataNode path="GEOM/SHANK/SHANK_INP" ioType="IN_TYPE" fiperType="FIPER_AUTOSHANK_PARAMETERS" 
      data="http://hippolyta.cs.ttu.edu/fiper/data/AS_Inp.dat"> INP </DataNode>
  </FiperContext>

  <FiperContext name="System Context">
    <TextNode path="is master"> true </TextNode>
    <TextNode path="job/strategy"> sequential </TextNode>
    <TextNode path="job/strategy/access"> catalog </TextNode>
    <TextNode path="notify/execution/to:/Kao"> kao@cs.ttu.edu </TextNode>
    <TextNode path="no wait"> false </TextNode>
    <TextNode path="in/task/id"> kao-task-autoshank-1 </TextNode>
    <TextNode path="out/task/id"> id </TextNode>
  </FiperContext>

  <FiperTask name="Generate Solid Shank"
    description="Generate a solid shank from\na parametric data file and a seed part"
    method="autoshank"
    provider="AUTOSHANK">
    <TaskContext root="shank"> GEOM/SHANK </TaskContext>   
    <TaskContext root="System Context"> </TaskContext>
	</FiperTask>
</FiperJob>
