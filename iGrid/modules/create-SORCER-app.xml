<!-- This scripts creates a full SORCER application shell with a sample
     provdier and requestor. It should be, baring any changes to SORCER,
     able to run once the app has been created and the provider and requestor
     build scripts have been executed.
     
     This script depends on template files in config/templates that have not
     been altered and that iGrid-home has been set.
     
     This script will run from the command line and provide command-line
     input prompts or run from Eclipse and provide a GUI input screens. 
     
     If you have any questions, please direct them to Michael Robbeloth -->
<project name="create-SORCER-app" default="create-module" basedir=".">
	<property environment="env" />
    <import file="${env.IGRID_HOME}/modules/common-build.xml" />
    <target name="create-module" 
             description="Create a project shell automatically">
         <!-- Ask user for name and type of module -->
         <input message="Name of Module:"
                addproperty="module.name"/>
         <input message="Type of Module [Eng(E), Example(X), Other(O)]:"
                addproperty="module.type"
                validargs="E,X,O"
         />
       
         <!-- Make sure user entered module name and type -->
         <fail message="Name and/or type not set!">
             <condition>
                 <or>
                     <isset property="${module.name}"/>
                     <isset property="${module.type}"/>
                 </or>
             </condition>
         </fail>
         
         <!-- Set module creation type based on specified module type -->
         <condition property="engineering.module">
               <or>
                     <contains string="E" substring="${module.type}"/>
               </or>
         </condition>
         
         <condition property="example.module">
               <or>
                     <contains string="X" substring="${module.type}"/>
               </or>
         </condition>
         
         <condition property="other.module">
               <or>
                     <contains string="O" substring="${module.type}"/>
               </or>
         </condition>   
         
         <antcall target="create-engineering-module" inheritAll="true"/>
         <antcall target="create-example-module" inheritAll="true"/>
         <antcall target="create-other-module" inheritAll="true"/>
     </target>
     
