<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<shortcuts>
    <skipIfNotSupported/>
    <programGroup defaultName="Sorcer" location="applications"/>
    <shortcut applications="yes" description="Boot Sorcer platform"
        desktop="yes" iconFile="$INSTALL_PATH/" iconIndex="0"
        initialState="minimized" name="Sorcer Boot" programGroup="no"
        startMenu="no" startup="no" target="$INSTALL_PATH/bin/sorcer-boot.bat"/>
    <shortcut applications="yes" desktop="yes"
        iconFile="$INSTALL_PATH/docs/sos.png" iconIndex="0"
        initialState="minimized" name="Sorcer Browser" programGroup="no"
        startMenu="no" startup="no" target="$INSTALL_PATH/bin/sorcer-browser.bat"/>
    <shortcut applications="yes" description="Sorcer Network Shell"
        desktop="yes" initialState="noShow" name="Network Shell"
        programGroup="no" startMenu="no" startup="no"
        target="$INSTALL_PATH/bin/nsh.bat" workingDirectory="$INSTALL_PATH"/>
</shortcuts>
